#!/bin/bash -norc

cat ~/.last_culess | sed 's/^RCS file: /0 -> WARNING MERGING:   &/g' | sed 's/^retrieving revision/0 -> WARNING MERGING:   &/g' | sed 's/^rcsmerge/0 -> WARNING MERGING:   &/g' | sed 's/^Merging/0 -> WARNING MERGING:   &/g' | sed 's/^cvs server: conflicts found in/0 -> WARNING MERGING:   &/g' | sed 's/^C /0 -> WARNING -- CONFLICT:      &/g' | sed 's/^? /0 -> CVS UNKNOWN:   &/g' | sed 's/^M /0 -> LOCALLY MODIFIED:   &/g' | sed 's/^A /0 -> TO BE ADDED TO CVS:   &/g' | sed 's/^R /0 -> TO BE REMOVED FROM CVS:   &/g' | sed 's/^cvs server: warning: /0 -> WARNING: &/g' | sed 's/^P /0 -> Z CVS UPDATING:   &/g' | sed 's/^U /0 -> Z NEW FILE FROM CVS:   &/g' | less 
