#!/bin/bash -norc

cat ~/.last_supless | sed 's/^RCS file: /0 -> WARNING MERGING:   &/g' | sed 's/^retrieving revision/0 -> WARNING MERGING:   &/g' | sed 's/^rcsmerge/0 -> WARNING MERGING:   &/g' | sed 's/^Merging/0 -> WARNING MERGING:   &/g' | sed 's/^svn server: conflicts found in/0 -> WARNING MERGING:   &/g' | sed 's/^C /0 -> WARNING -- CONFLICT:      &/g' | sed 's/^? /0 -> SVN UNKNOWN:   &/g' | sed 's/^M /0 -> LOCALLY MODIFIED:   &/g' | sed 's/^A /0 -> TO BE ADDED TO SVN:   &/g' | sed 's/^R /0 -> TO BE REMOVED FROM SVN:   &/g' | sed 's/^svn server: warning: /0 -> WARNING: &/g' | sed 's/^P /0 -> Z SVN UPDATING:   &/g' | sed 's/^U /0 -> Z NEW FILE FROM SVN:   &/g' | less 
